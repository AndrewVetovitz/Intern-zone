{
  "env": {
    "node": true,
    "es6": true
  },
    "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "impliedStrict": true
    }
  },
  "extends": "airbnb-base",
  "rules": {
    "indent": ["error", 4],
    "comma-dangle": ["error", "never"]
  }
}