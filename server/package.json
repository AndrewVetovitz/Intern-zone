{
    "name": "intern-zone",
    "description": "Intern Zone server",
    "version": "1.0.0",
    "author": "Andrew Vetovitz",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AndrewVetovitz/intern-zone.git"
    },
    "homepage": "https://github.com/AndrewVetovitz/intern-zone#readme",
    "bugs": {
        "url": "https://github.com/AndrewVetovitz/intern-zone/issues"
    },
    "main": "server.js",
    "scripts": {
        "start": "npm run serve",
        "scraper": "node dist/scraper-subprocess.js",
        "build": "npm run build-ts && npm run tslint",
        "serve": "node dist/server.js",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --verbose",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "dependencies": {
        "async": "~2.6.0",
        "bcrypt-nodejs": "~0.0.3",
        "bcryptjs": "~2.4.3",
        "bluebird": "~3.5.1",
        "body-parser": "~1.18.2",
        "compression": "~1.7.3",
        "cors": "~2.8.5",
        "dotenv": "~6.2.0",
        "errorhandler": "~1.5.0",
        "express": "~4.16.4",
        "express-flash": "0.0.2",
        "express-session": "~1.15.6",
        "express-validator": "~5.3.1",
        "helmet": "~3.15.0",
        "lodash": "~4.17.11",
        "lusca": "~1.6.1",
        "morgan": "~1.9.1",
        "mysql": "~2.16.0",
        "nodemailer": "~5.0.0",
        "passport": "~0.4.0",
        "passport-facebook": "~2.1.1",
        "passport-github": "~1.1.0",
        "passport-google-oauth": "~1.0.0",
        "passport-linkedin-oauth2": "~1.5.0",
        "passport-local": "~1.0.0",
        "reflect-metadata": "~0.1.12",
        "request": "~2.88.0",
        "socket.io": "~2.2.0",
        "typeorm": "~0.2.11"
    },
    "devDependencies": {
        "@types/async": "~2.0.45",
        "@types/bcrypt-nodejs": "~0.0.30",
        "@types/bcryptjs": "~2.4.2",
        "@types/bluebird": "~3.5.25",
        "@types/body-parser": "~1.17.0",
        "@types/compression": "~0.0.36",
        "@types/cors": "~2.8.4",
        "@types/dotenv": "~6.1.0",
        "@types/errorhandler": "~0.0.32",
        "@types/express": "~4.16.0",
        "@types/express-session": "~1.15.11",
        "@types/helmet": "0.0.42",
        "@types/jest": "~23.3.11",
        "@types/jquery": "~3.3.29",
        "@types/lodash": "~4.14.119",
        "@types/lusca": "~1.5.0",
        "@types/morgan": "~1.7.35",
        "@types/mysql": "~2.15.3",
        "@types/node": "~10.12.18",
        "@types/nodemailer": "~4.6.5",
        "@types/passport": "~1.0.0",
        "@types/passport-facebook": "~2.1.8",
        "@types/passport-github": "~1.1.3",
        "@types/passport-google-oauth": "~1.0.39",
        "@types/passport-linkedin-oauth2": "~1.5.0",
        "@types/passport-local": "~1.0.32",
        "@types/request": "~2.48.1",
        "@types/shelljs": "~0.8.1",
        "@types/socket.io": "~2.1.2",
        "@types/supertest": "~2.0.7",
        "chai": "~4.2.0",
        "concurrently": "~4.1.0",
        "jest": "~23.6.0",
        "nodemon": "~1.18.9",
        "shelljs": "~0.8.3",
        "supertest": "~3.3.0",
        "ts-jest": "~23.10.5",
        "ts-node": "~7.0.1",
        "tslint": "~5.12.0",
        "typescript": "~3.2.2"
    }
}
